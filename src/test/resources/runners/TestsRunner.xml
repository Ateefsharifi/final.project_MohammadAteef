<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="finalProjectMohammadAteef">
    <test name="CreatingPrimaryAccount">
        <classes>
            <class name="Tests.CreatePrimaryAccountPageTest">
                <methods>
                  <include name="createAccountWithValidEmail" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="creatingAccountSignUp">
        <classes>
            <class name="Tests.SignUpTest">
                <methods>
                    <include name="creatingAccountSignUp" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="navigateToLoginByValidCredential">
    <classes>
        <class name="Tests.LoginPageTest">
            <methods>
                <include name="navigateToLoginByValidCredential" />
            </methods>
        </class>
    </classes>
</test>
    <test name="navigateToLoginByInvalidCredential">
        <classes>
            <class name="Tests.LoginPageTest">
                <methods>
                    <include name="navigateToLoginByInvalidCredential" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="UserProfileDataValidation">
        <classes>
            <class name="Tests.PlanPageTest">
                <methods>
                    <include name="navigateToLoginByInvalidCredential" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="verifyAccountRecord">
        <classes>
            <class name="Tests.AccountTests">
                <methods>
                    <include name="navigateToLoginByInvalidCredential" />
                </methods>
            </class>
        </classes>
    </test>
</suite>